<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>guardarUsuario</servlet-name>
        <servlet-class>servlet.guardarUsuario</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>validarUsuario</servlet-name>
        <servlet-class>servlet.validarUsuario</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>actualizarP</servlet-name>
        <servlet-class>servlet.actualizarP</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>actualizarP1</servlet-name>
        <servlet-class>servlet.actualizarP1</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>actualizarP2</servlet-name>
        <servlet-class>servlet.actualizarP2</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>cerrar</servlet-name>
        <servlet-class>servlet.cerrar</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>guardarimc</servlet-name>
        <servlet-class>servlet.guardarimc</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>guardarUsuario</servlet-name>
        <url-pattern>/guardarUsuario</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>validarUsuario</servlet-name>
        <url-pattern>/validarUsuario</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>actualizarP</servlet-name>
        <url-pattern>/actualizarP</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>actualizarP1</servlet-name>
        <url-pattern>/actualizarP1</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>actualizarP2</servlet-name>
        <url-pattern>/actualizarP2</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>cerrar</servlet-name>
        <url-pattern>/cerrar</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>guardarimc</servlet-name>
        <url-pattern>/guardarimc</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
